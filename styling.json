{
  "version": "1.0",
  "themeOverrides": {
    "primaryColor": "customWhite",
    "secondaryColor": "customWite",
    "backgroundColor": "customWhite",
    "surfaceColor": "customWhite",
    "colors": {
      "customBlack": [
        "#000000",
        "#000000",
        "#000000",
        "#000000",
        "#000000",
        "#000000",
        "#000000",
