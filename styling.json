      ],
      "gray": [
        "#000000",
        "#000000",
        "#000000",
        "#000000",
        "#000000",
        "#000000",
        "#000000",
        "#000000",
        "#000000",
        "#000000"
      ]
    },
    "defaultRadius": "10",
    "fontFamily": "'Work Sans', sans-serif",
    "headings": {
      "fontFamily": "'Work Sans', sans-serif",
      "fontWeight": 300,
      "color": "#FFFFFF"
    },
    "white": "#FFFFFF",
    "black": "#000000",
    "components": {
      "Button": {
        "defaultProps": {
          "styles": {
            "root": {
              "backgroundColor": "#0055FF",
              "color": "#FFFFFF",
              "borderRadius": "8px",
              "border": "2px solid #0033AA",
              "fontWeight": 500,
              "padding": "10px 20px",
              "fontSize": "14px"
            }
          }
        }
      },
      "Slider": {
        "defaultProps": {
          "styles": {
            "track": {
              "backgroundColor": "#333333",
              "height": "4px"
            },
            "bar": {
              "backgroundColor": "#0055FF"
            },
            "thumb": {
              "border": "2px solid #0055FF",
              "backgroundColor": "#FFFFFF",
              "height": "18px",
              "width": "18px"
            },
            "label": {
              "backgroundColor": "#0055FF",
              "color": "#FFFFFF"
            }
          }
        }
      },
      "Menu": {
        "defaultProps": {
          "styles": {
            "dropdown": {
              "backgroundColor": "#1E1E1E",
              "borderRadius": "8px",
              "border": "1px solid #333333",
              "padding": "8px"
            },
            "item": {
              "color": "#FFFFFF",
              "fontSize": "14px",
              "padding": "6px 12px",
              "&:hover": {
                "backgroundColor": "#0055FF",
                "color": "#FFFFFF"
              }
            },
            "label": {
              "color": "#AAAAAA",
              "fontSize": "12px",
              "fontWeight": "500"
            }
          }
        }
      }
    }
  }
}